{
  "family": "akiverse-production-fee-collector",
  "containerDefinitions": [
    {
      "name": "akiverse-fee-collector",
      "image": "",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awsfirelens",
        "secretOptions": [],
        "options": null
      },
      "command": ["node", "fee_collector.js"],
      "environment": [
        {
          "name": "ENV",
          "value": "production"
        }
      ],
      "secrets": [
        {
          "name": "SENTRY_DSN",
          "valueFrom": "arn:aws:ssm:ap-south-1:922777733047:parameter/production/SENTRY_DSN"
        },
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:ssm:ap-south-1:922777733047:parameter/production/DATABASE_URL"
        }
      ]
    },
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": [],
        "options": {
          "awslogs-group": "/ecs/production/fee-collector",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "firelens"
        }
      },
      "environment": [
        {
          "name": "aws_fluent_bit_init_s3_1",
          "value": "arn:aws:s3:::akiverse-production-firelens-config-708de480a7619757/production/fee-collector/parser.conf"
        },
        {
          "name": "aws_fluent_bit_init_s3_2",
          "value": "arn:aws:s3:::akiverse-production-firelens-config-708de480a7619757/production/fee-collector/extra.conf"
        }
      ],
      "image": "public.ecr.aws/aws-observability/aws-for-fluent-bit:init-latest",
      "firelensConfiguration": {
        "type": "fluentbit",
        "options": {
          "enable-ecs-log-metadata": "true"
        }
      },
      "name": "fluentbit"
    }
  ],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "akiverse-production-task-execution",
  "taskRoleArn": "akiverse-production-task-execution",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX"
  }
}
